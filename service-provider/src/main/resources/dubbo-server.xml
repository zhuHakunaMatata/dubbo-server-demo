<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

        <!-- service provider -->
        <dubbo:application name="dubbo-server"></dubbo:application>

        <!-- 注册中心 -->
        <!--<dubbo:registry address="zookeeper://192.168.5.129:2181"/>-->
        <dubbo:registry address="zookeeper://192.168.5.131:2181"/>


    <!-- service & Bean -->
        <dubbo:service interface="com.my.service.IHelloService"
                ref="helloService" ></dubbo:service>

        <bean id="helloService" class="com.my.service.HelloServiceImpl"></bean>


</beans>